@model AdoptifySystem.Event_Type

@{
    ViewBag.Title = "Details";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Event Type Details</h2>

<div>
    <h4>Event Type</h4>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            Event Type Name:
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Event_Type_Name)
        </dd>

    </dl>
</div>
<p>
    @Html.ActionLink("Update", "Edit", new { id = Model.Event_Type_ID }) |
    @Html.ActionLink("Back", "Index")
</p>
