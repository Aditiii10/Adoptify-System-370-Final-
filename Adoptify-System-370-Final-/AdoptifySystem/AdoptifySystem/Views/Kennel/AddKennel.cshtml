
@{
    ViewBag.Title = "AddKennel";
}

<head>
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.4.1/jquery.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.14.7/umd/popper.min.js"></script>
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/4.3.1/js/bootstrap.min.js"></script>
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.4.1/jquery.min.js"></script>
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.4.0/js/bootstrap.min.js"></script>

</head>

<div>
    <div class="col-sm-10" style='background-color: #2ba6cb'><h1 style="color:white;">Add Kennel</h1></div>

</div>
@using (Html.BeginForm()) { 
if (Model != null)
{
    using (Html.BeginForm())
    {

            <label>
                Enter Kennel Name
            </label>
            <br />
            <input name="Kennel_Name" type="text" />
            <br />
            <label>
                Enter Kennel Number
            </label>
            <br />
            <input name="Kennel_Number" type="text" />
            <br />
            <label>
                Enter Kennel Capacity
            </label>
            <br />
            <input name="Kennel_Capacity" type="text" />
            <br />
            <div class="form-group">
                <div class="col-md-offset-2 col-md-10">
                    <button type="submit" name="button" value="Cancel" class="btn btn-warning">Cancel</button>
                    <button type="submit" name="button" value="Save" class=" btn btn-success float-right">Save</button>
                </div>
            </div>
        }
    }
    else
    {
        <h2>the Title is empty</h2>
    }
}